<span class="kd">let</span> <span class="nx">secretSpendKey</span> <span class="o">=</span> <span class="kc">null</span><span class="p">;</span>
<span class="kd">let</span> <span class="nx">publicSpendKey</span> <span class="o">=</span> <span class="kc">null</span><span class="p">;</span>

<span class="nx">walletd</span><span class="p">.</span><span class="nx">createAddress</span><span class="p">(</span><span class="nx">secretSpendKey</span><span class="p">,</span> <span class="nx">publicSpendKey</span><span class="p">)</span>
